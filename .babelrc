{
  "presets": [
    [
      "@babel/preset-env", {
        "useBuiltIns": "usage",
        "debug": false
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "transform-class-properties",
    "@babel/plugin-proposal-export-default-from",
    [
      "@babel/plugin-proposal-decorators", {
      "legacy": true
    }
    ],
    [
      "import", {
      "libraryName": "antd"
    },
      "antd"
    ]
  ]
}